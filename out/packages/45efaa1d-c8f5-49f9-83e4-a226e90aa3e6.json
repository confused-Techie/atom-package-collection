        "name": "linter-tidy",
        "main": "./lib/main.js",
        "version": "2.4.0",
        "description": "Linter plugin for HTML, using tidy",
        "repository": {
            "type": "git",
            "url": "https://github.com/AtomLinter/linter-tidy"
        },
        "keywords": [
            "linter",
            "lint",
            "tidy",
