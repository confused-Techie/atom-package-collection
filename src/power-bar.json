      "dependencies": {},
      "configSchema": {
        "height": {
          "title": "Bar height",
          "default": 6,
          "minimum": 1,
          "maximum": 32,
          "type": "number"
        },
        "position": {
          "title": "Position",
          "description": "Position for the battery level display bar",
          "type": "string",
          "default": "header",
          "enum": [
            "header",
            "top",
            "bottom",
            "footer"
          ]
        },
        "colors": {
          "title": "Bar coloring",
          "type": "object",
          "properties": {
            "low": {
              "order": 1,
              "title": "Low battery color",
              "default": "#af4c4c",
              "type": "color"
            },
            "mid": {
              "order": 2,
              "title": "Battery warning",
              "default": "#af4c4c",
              "type": "color"
            },
            "main": {
              "order": 3,
              "title": "Plenty o' battery",
              "default": "#af4c4c",
              "type": "color"
            },
            "high": {
              "order": 4,
              "title": "Full battery color",
              "default": "#af4c4c",
              "type": "color"
            }
          }
        },
        "visible": {
          "title": "Show power level",
          "type": "boolean",
          "default": true
        },
        "label-visible": {
          "title": "Show label",
          "description": "Show numeral label for power level",
          "type": "boolean",
          "default": true
        }
      },
      "eslintConfig": {
