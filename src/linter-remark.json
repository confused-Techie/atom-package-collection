      },
      "package-deps": [
        "linter"
      ],
      "dependencies": {
        "atom-package-deps": "^7.0.0",
        "remark": "^13.0.0",
        "unified-engine-atom": "^9.0.0"
      },
      "devDependencies": {
        "atom-tap-test-runner": "^7.0.0",
        "prettier": "^2.0.0",
        "remark-cli": "^9.0.0",
        "remark-preset-wooorm": "^8.0.0",
        "tape": "^5.0.0",
        "xo": "^0.34.0"
      },
      "scripts": {
        "format": "remark . -qfo --ignore-pattern test/ && prettier . -w --loglevel warn && xo --fix",
        "test-api": "atom --test test/index.js",
        "test": "npm run format && npm run test-api"
      },
      "prettier": {
        "tabWidth": 2,
        "useTabs": false,
        "singleQuote": true,
        "bracketSpacing": false,
        "semi": false,
        "trailingComma": "none"
      },
      "xo": {
        "prettier": true,
        "esnext": false,
        "rules": {
          "unicorn/no-fn-reference-in-iterator": "off"
        }
      },
      "remarkConfig": {
        "plugins": [
          "preset-wooorm"
        ]
      },
      "atomTestRunner": "atom-tap-test-runner",
      "providedServices": {
        "linter": {
          "versions": {
            "2.0.0": "provideLinter"
          }
        }
      },
      "configSchema": {
        "detectConfig": {
          "title": "Config files",
          "description": "Use `.remarkrc` files and `remarkConfig` in `package.json` files.",
          "type": "boolean",
          "default": true
        },
        "detectIgnore": {
          "title": "Ignore files",
          "description": "Use `.remarkignore` files.",
          "type": "boolean",
          "default": true
        },
        "scopes": {
          "title": "Scopes",
          "description": "List of scopes for languages which will be checked (run `Editor: Log Cursor Scope` to determine the scopes for a file).",
          "type": "array",
          "items": {
            "type": "string"
          },
          "default": [
            "source.gfm",
            "source.pfm",
            "text.md"
          ]
        }
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/linter-remark/versions/9.0.0/tarball"
      }
    },
