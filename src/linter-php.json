            ]
          }
        ]
      },
      "env": {
        "node": true,
        "browser": true
      },
      "globals": {
        "atom": true
      }
    },
    "renovate": {
      "extends": [
        "config:base"
      ],
      "semanticCommits": true,
      "rangeStrategy": "pin",
      "packageRules": [
        {
          "packagePatterns": [
            "^eslint"
          ],
          "groupName": "ESLint packages"
        }
      ]
    },
    "release": {
      "extends": "@semantic-release/apm-config"
    },
    "commitlint": {
      "extends": [
        "@commitlint/config-conventional"
      ]
    }
  }
}