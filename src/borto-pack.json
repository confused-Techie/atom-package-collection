      "etch": "^0.14.0",
      "pump": "^3.0.0",
      "through2": "^3.0.1"
    },
    "consumedServices": {
      "busy-signal": {
        "versions": {
          "1.0.0": "consumeSignal"
        }
      }
    },
    "license": "MIT"
  }
