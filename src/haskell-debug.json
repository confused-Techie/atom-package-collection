      "scripts": {
        "build": "tsc -p lib-src && tsc -p bin-src",
        "prettier": "prettier --write '*-src/**/*.ts?(x)' 'spec/**/*.ts?(x)'",
        "prettier-check": "prettier -l '*-src/**/*.ts?(x)' 'spec/**/*.ts?(x)'",
        "typecheck": "tsc --noEmit -p lib-src && tsc --noEmit -p bin-src && tsc --noEmit -p spec",
