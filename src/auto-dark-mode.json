    "keywords": [
      "dark mode",
      "themes",
      "switch",
      "macOS",
      "atom"
    ],
    "activationCommands": {},
    "repository": "https://github.com/paysonwallach/auto-dark-mode",
    "license": "MIT",
    "engines": {
      "atom": ">=1.52.0 <2.0.0"
    },
    "configSchema": {
      "darkProfile": {
        "order": 1,
        "description": "UI and syntax theme for dark mode",
        "type": "string",
        "default": "one-dark-ui one-dark-syntax"
      },
      "lightProfile": {
        "order": 2,
        "description": "UI and syntax theme for light mode",
        "type": "string",
        "default": "one-light-ui one-light-syntax"
      },
      "notifyOnChange": {
