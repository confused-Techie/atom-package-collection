    "1.0.5": {
      "name": "tabs-to-spaces",
      "author": "Lee Dohm <lee@lee-dohm.com>",
      "main": "./lib/index",
      "version": "1.0.5",
      "description": "Provides the ability to convert between leading tabs and spaces in a document",
      "keywords": [
        "convert",
        "indentation",
        "spaces",
        "tabs"
      ],
      "repository": "https://github.com/lee-dohm/tabs-to-spaces",
      "license": "MIT",
      "engines": {
        "atom": ">=1.26.0 <2.0.0"
      },
      "devDependencies": {
        "rimraf": "~2.5",
        "temp": "~0.6.0"
      },
      "configSchema": {
        "onSave": {
          "type": "string",
          "default": "none",
          "enum": [
            "none",
            "tabify",
            "untabify"
          ],
          "description": "Setting this to anything other than `none` can **significantly** impact the time it takes to save large files."
        }
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/tabs-to-spaces/versions/1.0.5/tarball"
      }
    },
