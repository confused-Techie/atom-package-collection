        "rollup-plugin-coffee-script": "^2.0.0",
        "rollup-plugin-terser": "^7.0.2"
      },
      "lint-staged": {
        "*.(coffee|cson|json)": "eslint --cache --fix"
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/language-bridlensis/versions/5.1.0/tarball"
      }
    },
