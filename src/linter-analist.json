      "repository": "https://github.com/tcoenraad/linter-analist",
      "license": "MIT",
      "engines": {
        "atom": ">=1.4.0 <2.0.0"
      },
      "activationHooks": [
        "language-ruby:grammar-used",
        "language-ruby-on-rails:grammar-used"
      ],
      "configSchema": {
        "command": {
          "type": "string",
          "default": "analist",
          "description": "This is the absolute path to your `analist` command. You may need to run `which analist` or `rbenv which analist` to find this. Examples: `/usr/local/bin/analist` or `/usr/local/bin/bundle exec analist --config /my/analist.yml`."
        },
        "ignoredExtensions": {
          "type": "array",
          "default": [
            "erb",
            "md"
          ],
          "items": {
            "type": "string"
          }
        },
        "slackLogWebhook": {
          "type": "string",
          "default": "https://hooks.slack.com/services/",
          "description": "This is the Slack Webhook to which logging is sent."
        }
      },
      "providedServices": {
        "linter": {
          "versions": {
            "1.0.0": "provideLinter"
          }
        }
      },
      "dependencies": {
        "atom-linter": "^10.0.0",
        "atom-package-deps": "^4.0.1",
        "request": "^2.83.0",
        "username": "^3.0.0"
      },
      "devDependencies": {
        "eslint": "^4.6.0",
        "eslint-config-airbnb-base": "^12.0.0",
        "eslint-plugin-import": "^2.7.0",
        "jasmine-fix": "^1.3.1"
      },
      "package-deps": [
        "linter"
      ],
      "scripts": {
        "lint": "eslint .",
        "test": "apm test"
      },
      "eslintConfig": {
        "extends": "airbnb-base",
        "rules": {
          "global-require": "off",
          "import/no-unresolved": [
            "error",
            {
              "ignore": [
                "atom"
              ]
            }
          ]
        },
        "env": {
          "browser": true,
          "node": true
        },
        "globals": {
          "atom": true
        }
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/linter-analist/versions/1.2.2/tarball"
      }
