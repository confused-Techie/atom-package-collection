  "versions": {
    "0.17.2": {
      "name": "atom-fuzzy-grep",
      "main": "./lib/atom-fuzzy-grep",
      "version": "0.17.2",
      "description": "Fuzzy grep project using ag, pt, ack, grep, ripgrep, or git-grep",
      "keywords": [
        "fuzzy",
        "grep",
        "ag",
        "pt",
        "productivity",
        "search",
        "find"
      ],
      "configSchema": {
        "minSymbolsToStartSearch": {
          "type": "number",
          "default": 3,
          "order": 0
        },
        "maxCandidates": {
          "type": "number",
          "default": 100,
          "order": 1
        },
        "grepCommandString": {
          "type": "string",
          "default": "ag -i --nocolor --nogroup --column",
          "order": 2
        },
        "detectGitProjectAndUseGitGrep": {
          "type": "boolean",
          "default": true,
          "order": 3
        },
        "gitGrepCommandString": {
          "type": "string",
          "default": "git grep -i --no-color -n -E",
          "order": 4
        },
        "preserveLastSearch": {
          "type": "boolean",
          "default": false,
          "order": 5
        },
        "escapeSelectedText": {
          "type": "boolean",
          "default": true,
          "order": 6
        },
        "showFullPath": {
          "type": "boolean",
          "default": false,
          "order": 7
        },
        "inputThrottle": {
          "type": "integer",
          "default": 50,
          "order": 8
        },
        "escapeOnPaste": {
          "type": "boolean",
          "default": true,
          "order": 9
        },
        "fixEnv": {
          "type": "boolean",
          "description": "Fix env on OS X when running not from terminal.",
          "default": true,
          "order": 10
        }
      },
      "activationCommands": {
        "atom-workspace": [
          "fuzzy-grep:toggle",
          "fuzzy-grep:toggleLastSearch",
          "fuzzy-grep:toggleWordUnderCursor"
        ]
      },
      "repository": "https://github.com/geksilla/atom-fuzzy-grep",
      "license": "MIT",
      "engines": {
        "atom": ">=0.174.0 <2.0.0"
      },
      "dependencies": {
        "space-pen": "^5.1.1",
        "atom-space-pen-views": "^2.0.0",
        "escape-string-regexp": "^1.0.3",
        "fuzzaldrin": "^2.1.0"
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/atom-fuzzy-grep/versions/0.17.2/tarball"
      }
    },
