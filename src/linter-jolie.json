      "main": "./lib/main",
      "configSchema": {
        "jolieExecutablePath": {
          "type": "string",
          "default": "jolie",
          "title": "Jolie Executable Path",
          "description": "The executable path to Jolie. In most systems, the default works if Jolie is in your PATH environment variable. If it does not work for you, put the full path here, e.g., /usr/bin/jolie"
        }
      },
      "dependencies": {
