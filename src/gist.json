    "1.1.0": {
      "name": "gist",
      "main": "./lib/main",
      "version": "1.1.0",
      "description": "create and insert Gists.",
      "keywords": [
        "github",
        "gist"
      ],
      "activationCommands": {
        "atom-text-editor:not([mini])": [
          "gist:create-public",
          "gist:create-private",
          "gist:list"
        ]
      },
      "repository": "https://github.com/aki77/atom-gist",
      "license": "MIT",
      "engines": {
        "atom": ">=1.4.0 <2.0.0"
      },
      "dependencies": {
        "@aki77/atom-input-dialog": "^1.0.0",
        "@aki77/atom-select-action": "^0.1.2",
        "atom-space-pen-views": "^2.0.5",
        "bluebird": "^3.1.4",
        "fuzzaldrin": "^2.1.0",
        "octonode": "^0.7.1",
        "temp": "~0.8.1",
        "underscore-plus": "^1.6.6",
        "untildify": "^3.0.2"
      },
      "configSchema": {
        "token": {
          "order": 1,
          "type": "string",
          "default": "",
          "description": "github personal access token"
        },
        "tokenFile": {
          "order": 2,
          "type": "string",
          "default": "~/.atom/gist.token",
          "description": "not save the token to the configuration file"
        },
        "environmentName": {
          "order": 3,
          "type": "string",
          "default": "GIST_ACCESS_TOKEN",
          "description": "not save the token to the configuration file"
        },
        "closeOnSave": {
          "order": 4,
          "type": "boolean",
          "default": false,
          "description": "close the tab when you save a gist"
        },
        "hostname": {
          "order": 5,
          "type": "string",
          "default": "api.github.com",
          "description": "api endpoint, like api.github.com or githubenterprise/api/v3"
        },
        "debug": {
          "order": 99,
          "type": "boolean",
          "default": false
        }
      },
      "dist": {
        "tarball": "https://www.atom.io/api/packages/gist/versions/1.1.0/tarball"
      }
    },
