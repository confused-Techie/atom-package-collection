    "name": "jshint",
    "version": "1.8.6",
    "description": "Validate JavaScript with JSHint. In realtime or on save. Supports JSX (React).",
    "license": "MIT",
    "repository": "sindresorhus/atom-jshint",
    "private": true,
    "author": {
      "name": "Sindre Sorhus",
      "email": "sindresorhus@gmail.com",
      "url": "sindresorhus.com"
    },
    "consumedServices": {
      "status-bar": {
        "versions": {
          "^1.0.0": "consumeStatusBar"
        }
      }
    },
    "engines": {
      "atom": ">=1.0.11"
    },
    "scripts": {
      "test": "xo"
    },
    "keywords": [
      "lint",
      "javascript",
      "validate",
      "jshint",
      "jsx"
    ],
    "dependencies": {
      "event-kit": "^1.2.0",
      "jshint": "^2.4.4",
      "jshint-jsx": "^0.4.0",
      "lazy-req": "^1.0.0",
      "lodash": "^3.10.0",
      "shelljs": "^0.5.0",
      "strip-json-comments": "^1.0.1",
      "user-home": "^2.0.0"
